page {
  background: #f5f5f5;
}
.loading-l1 {
  height: 80rpx;
  padding: 0 20rpx;
  margin-top: 15rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  font-size: 30rpx;
  color: #858585;
}
.loading-l1-v1 {
  display: flex;
  align-items: center;
}
.loading-l1-v1-h1 {
  width: 18rpx;
  height: 30rpx;
  transform: rotate(180deg);
}
.loading-l1-v1-h3 {
  width: 18rpx;
  height: 30rpx;
}
.loading-l1-v1-h2 {
  width: 180rpx;
  text-align: center;
}
.loading-l2 {
  height: 800rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.loading-l2-01 {
  border: 2rpx solid #07c160;
  border-bottom-color: transparent;
  border-right-color: transparent;
  border-radius: 50%;
  animation: 1s loader-01 linear infinite;
  width: 200rpx;
  height: 200rpx;
  background: #fff;
}
@keyframes loader-01 {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}
.loading-l2-02{
  border: 2rpx solid transparent;
  border-bottom-color: #07c160;
  border-top-color: #07c160;
  border-radius: 50%;
  animation: 1s loader-01 linear infinite;
  width: 200rpx;
  height: 200rpx;
  background: #fff;
}
.loading-l2-03{
  border-radius: 50%;
  animation: 1s loader-03 ease-out infinite;
  width: 200rpx;
  height: 200rpx;
  position: relative;
  background: #fff;
}
@keyframes loader-03{
  0% {
        transform: scale(0);
        opacity: 0;
    }
    50% {
        opacity: 1;
    }
    100% {
        transform: scale(1.2);
        opacity: 0;
    }
}
.loading-l2-04{
  width: 50rpx;
  height: 50rpx;
  position: relative;
  animation: loading-04 1s infinite linear;
}
@keyframes loading-04{
  100%{
    transform: rotate(360deg);
  }
}
.loading-l2-04-h1{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: #189de4;
  position: absolute;
}
.loading-l2-04-h1:nth-child(1){
  top: 0;
  left: 0;
}
.loading-l2-04-h1:nth-child(2){
  top: 0;
  right: 0;
  opacity: 0.7
}
.loading-l2-04-h1:nth-child(3){
  opacity: 0.5;
  bottom: 0;
  left: 0;
}
.loading-l2-04-h1:nth-child(4){
  bottom: 0;
  right: 0;
  opacity: 0.3;
}
.loading-l2-05{
  display: flex;
  justify-content: space-around;
  color: #189de4;
  font-size: 30rpx;
}
.loading-l2-05-h1{
  margin: 0 15rpx;
  animation: loading05 .6s infinite alternate;
}
.loading-l2-05-h1:nth-child(2) {
    animation-delay: 0.1s;
}
.loading-l2-05-h1:nth-child(3) {
    animation-delay: 0.2s;
}
.loading-l2-05-h1:nth-child(4) {
    animation-delay: 0.3s;
}
.loading-l2-05-h1:nth-child(5) {
    animation-delay: 0.4s;
}
.loading-l2-05-h1:nth-child(6) {
    animation-delay: 0.5s;
}
@keyframes loading05 {
    0% {
        transform: scale(2);
    }
    100% {
        transform: scale(1);
    }
}
.loading-l2-05-h2{
  margin: 0 15rpx;
  animation: loading06 .6s infinite alternate;
}
.loading-l2-05-h2:nth-child(2) {
    animation-delay: 0.1s;
}
.loading-l2-05-h2:nth-child(3) {
    animation-delay: 0.2s;
}
.loading-l2-05-h2:nth-child(4) {
    animation-delay: 0.3s;
}
.loading-l2-05-h2:nth-child(5) {
    animation-delay: 0.4s;
}
.loading-l2-05-h2:nth-child(6) {
    animation-delay: 0.5s;
}
@keyframes loading06 {
    0% {
        transform: translateY(0rpx)
    }
    100% {
        transform: translateY(15rpx);
    }
}